<script>
    function toHHMMSS(secs) {
        var sec_num = parseInt(secs, 10)
        var hours = Math.floor(sec_num / 3600)
        var minutes = Math.floor(sec_num / 60) % 60
        var seconds = sec_num % 60

        return [hours, minutes, seconds]
            .map(v => v < 10 ? "0" + v : v)
            .filter((v, i) => v !== "00" || i > 0)
            .join(":")
    }
    function getWorkingTime() {
        var startDate = new Date("<%- statuslastchange %>");
        var currentDate = new Date();
        var WorkingSeconds = (currentDate.getTime() - startDate.getTime()) / 1000;
        document.getElementById('WORK_E').innerHTML = toHHMMSS(WorkingSeconds);
        var t = setTimeout(getWorkingTime, 500);
    }
</script>